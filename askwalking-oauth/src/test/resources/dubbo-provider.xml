<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd          
    http://code.alibabatech.com/schema/dubbo          
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:provider filter="traceNoFilter"/>

    <dubbo:application name="${dubbo.application.name}"/>
    <dubbo:consumer retries="${dubbo.retries}" timeout="${dubbo.timeout}" check="false"/>
    <dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}" check="true"/>
    <dubbo:protocol name="dubbo" port="${dubbo.port}"/>

                            <!-- 对外提供服务的接口 -->
    <!--保证金账户操作API -->
    <dubbo:service interface="com.sinoiov.etc.uid.generator.api.UidGeneratorOperatorService"
                   ref="uidGeneratorOperatorService" timeout="${dubbo.timeout}" retries="${dubbo.retries}"/>

</beans>

